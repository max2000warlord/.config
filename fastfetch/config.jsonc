{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    "source": "/home/max/.config/fastfetch/artix.png",
    "width": 35,
    "padding": {
      "top": 0,
      "right": 2,
      "left": 0
    }
    //"height": 10
  },
  "display": {
    "separator": "  "
  },
  "modules": [
    {
      "type": "custom", // HardwareStart
      "format": "\u001b[38;2;16;175;223m╭───────┤ \u001b[1;34mHardware Information\u001b[38;2;16;175;223m ├───────╮\u001b[0m"
      // \u001b[1;34m sets the text to bold blue, \u001b[0m resets the color
    },
    {
      "type": "cpu",
      "key": "\u001b[38;2;255;0;0m󰻠\u001b[0m"
    },
    {
      "type": "disk",
      "key": "\u001b[1;32m󰋊\u001b[0m" // Green color for disk icon,
    },
    {
      "type": "memory",
      "key": "\u001b[1;33m󰑭\u001b[0m" // Yellow color for memory icon
    },
    {
      "type": "display",
      "key": "󰍹"
      //"format": "{1}×{2} @ {3}Hz"
    },
    {
      "type": "battery",
      "key": ""
    },
    {
      "type": "custom", // SoftwareStart
      "format": "\u001b[38;2;16;175;223m├───────┤ \u001b[1;34mSoftware Information\u001b[38;2;16;175;223m ├───────┤\u001b[0m"
    },
    {
      "type": "title",
      "key": "",
      "format": "{1}@{2}"
    },
    {
      "type": "os",
      "key": "\u001b[38;2;16;175;223m\u001b[0m" //Just get your distro icon from https://www.nerdfonts.com/cheat-sheet
    },
    {
      "type": "initsystem",
      "key": ""
    },
    {
      "type": "kernel",
      "key": "\u001b[38;2;255;255;255m\u001b[0m" //Just get your distro icon from https://www.nerdfonts.com/cheat-sheet
    },
    {
      "type": "wm",
      "key": "",
      "format": "{1}"
    },
    {
      "type": "shell",
      "key": ""
    },
    {
      "type": "terminal",
      "key": "",
      "format": "{5}"
    },
    {
      "type": "packages",
      "key": "\u001b[38;2;255;204;0m󰮯\u001b[0m",
      "format": "{1}"
    },
    {
      "type": "uptime",
      "key": "󰅐"
    },
    {
      "type": "wifi",
      "key": ""
    },
    {
      "type": "locale",
      "key": ""
    },
    {
      "type": "custom", // InformationEnd
      "format": "\u001b[38;2;16;175;223m╰──────────────────────────────────────╯\u001b[0m\n"
    }
  ]
}
